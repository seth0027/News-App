{
  "expo": {
    
    "name": "news-app",
    "slug": "news-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/news-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/news-icon.png",
      "resizeMode": "300x300",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/news-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
